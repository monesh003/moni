import java.util.*;
public class MyClass {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int dig,rev=0;
        int num=sc.nextInt();
        while(num!=0){
            dig=num%10;
            rev=rev*10+dig;
            num/=10;
            System.out.println(rev);
        }
    }
}
