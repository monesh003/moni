import java.util.*;
public class MyClass {
    public static void main(String args[]) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int i,a=0,b=0;
        for(i=1;i<=n;i++){
            if(i%2!=0) a+=7;
            else b+=6;
        if(i%2!=0) System.out.print(a-7+" ");
        else System.out.print(b-6+" ");
        }
    }
}
